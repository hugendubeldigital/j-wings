package org.wings;

/**
 * This class provides version and compile time strings. It is generated
 * at build time.
 */
public class Version {
    public static String getVersion() { return "@WINGS_VERSION@"; }
    public static String getCompileTime() { return "@COMPILE_TIME@"; }
}