<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">

<web-app>
  <servlet>
    <servlet-name>
      BeanEditor
    </servlet-name>
    <servlet-class>
      beaneditor.BeanEditor
    </servlet-class>
  </servlet>

<!-- url mapping -->

  <servlet-mapping>
    <servlet-name>
      Beaneditor
    </servlet-name>
    <url-pattern>
      /Beaneditor/*
    </url-pattern>
  </servlet-mapping>

</web-app>
