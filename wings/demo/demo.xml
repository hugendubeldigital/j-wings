<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">

<web-app>
    <servlet>
        <servlet-name>
            WingSet
        </servlet-name>
        <servlet-class>
            wingset.WingSet
        </servlet-class>
        <init-param>
            <param-name>externalizer.servlet.url</param-name>
            <param-value>ExternalizerServlet</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>
            ServerExplorer
        </servlet-name>
        <servlet-class>
            explorer.Explorer
        </servlet-class>
        <init-param>
            <param-name>externalizer.servlet.url</param-name>
            <param-value>ExternalizerServlet</param-value>
        </init-param>
        <init-param>
            <param-name>ExplorerBaseDir</param-name>
            <param-value>/var</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>
            LdapClient
        </servlet-name>
        <servlet-class>
           ldap.LdapClient
        </servlet-class>
        <init-param>
            <param-name>externalizer.servlet.url</param-name>
            <param-value>ExternalizerServlet</param-value>
        </init-param>
        <init-param>
            <param-name>ldap.server.host</param-name>
            <param-value>192.168.230.131:389</param-value>
        </init-param>
        <init-param>
            <param-name>ldap.server.basedn</param-name>
            <param-value>dc=tiscon,dc=de</param-value>
        </init-param>
        <init-param>
            <param-name>ldap.server.binddn</param-name>
            <param-value>cn=admin,dc=tiscon,dc=de</param-value>
        </init-param>
         </servlet>
    <servlet>
        <servlet-name>
            LdapBrowser
        </servlet-name>
        <servlet-class>
           ldap.LdapBrowser
        </servlet-class>
        <init-param>
            <param-name>externalizer.servlet.url</param-name>
            <param-value>ExternalizerServlet</param-value>
        </init-param>
        <init-param>
            <param-name>ldap.server.host</param-name>
            <param-value>192.168.230.131:389</param-value>
        </init-param>
        <init-param>
            <param-name>ldap.server.basedn</param-name>
            <param-value>dc=tiscon,dc=de</param-value>
        </init-param>
        <init-param>
            <param-name>ldap.server.binddn</param-name>
            <param-value>cn=admin,dc=tiscon,dc=de</param-value>
        </init-param>
        <init-param>
            <param-name>ldap.server.peoplename</param-name>
            <param-value>cn</param-value>
        </init-param>

        </servlet>
    <servlet>
        <servlet-name>
            TableExample2
        </servlet-name>
        <servlet-class>
            jfc.TableExample.TableExample2
        </servlet-class>
        <init-param>
            <param-name>externalizer.servlet.url</param-name>
            <param-value>ExternalizerServlet</param-value>
        </init-param>
    </servlet>
    <servlet>
      <servlet-name>
          ExternalizerServlet
      </servlet-name>
      <servlet-class>
          org.wings.externalizer.ExternalizerServlet
      </servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>
            WingSet
        </servlet-name>
        <url-pattern>
            /WingSet
        </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>
            ServerExplorer
        </servlet-name>
        <url-pattern>
            /Explorer
        </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>
            LdapClient
        </servlet-name>
        <url-pattern>
            /LdapClient
        </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>
            LdapBrowser
        </servlet-name>
        <url-pattern>
            /LdapBrowser
        </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>
            TableExample2
        </servlet-name>
        <url-pattern>
            /TableExample2
        </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>
            ExternalizerServlet
        </servlet-name>
        <url-pattern>
            /ExternalizerServlet/*
        </url-pattern>
    </servlet-mapping>
</web-app>
