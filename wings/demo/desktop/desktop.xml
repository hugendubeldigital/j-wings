<?xml version="1.0" encoding="ISO-8859-1"?>

<!--DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd"-->

<web-app>
  <servlet>
    <servlet-name>
      Desktop
    </servlet-name>
    <servlet-class>
      desktop.Desktop
    </servlet-class>
    <init-param>
      <param-name>externalizer.servlet.url</param-name>
      <param-value>ExternalizerServlet</param-value>
    </init-param>
  </servlet>

  <servlet>
    <servlet-name>
      ExternalizerServlet
    </servlet-name>
    <servlet-class>
      org.wings.externalizer.ExternalizerServlet
    </servlet-class>
  </servlet>

<!-- url mappings -->

  <servlet-mapping>
    <servlet-name>
      Desktop
    </servlet-name>
    <url-pattern>
      /Desktop
    </url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>
      ExternalizerServlet
    </servlet-name>
    <url-pattern>
      /ExternalizerServlet
    </url-pattern>
  </servlet-mapping>
</web-app>
