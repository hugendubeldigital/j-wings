<?xml version="1.0" encoding="ISO-8859-1"?>

<!--DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd"-->

<web-app>
  <servlet>
    <servlet-name>
      FrameSet
    </servlet-name>
    <servlet-class>
      frameset.FrameSet
    </servlet-class>
  </servlet>

<!-- url mappings -->

  <servlet-mapping>
    <servlet-name>
      FrameSet
    </servlet-name>
    <url-pattern>
      /FrameSet/*
    </url-pattern>
  </servlet-mapping>
</web-app>
