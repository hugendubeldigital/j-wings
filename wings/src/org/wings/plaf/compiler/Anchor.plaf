<template name="Anchor" for="org.wings.SAnchor">
<%@ include file="common.plaf"%>

<write>
<%
SBorder border = component.getBorder();
renderBorderPrefix(border);
%><a<% 
printOptAttr("name", component.getName());
printOptAttr("href", component.getReference());
%>><%
renderContainer(component);
%></a><%
renderBorderPrefix(border);
%>
</write>
</template>
