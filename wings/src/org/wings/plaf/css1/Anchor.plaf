<template name="AnchorCG" for="org.wings.SAnchor">

<write>
<a<%
   final String style = (component.getStyle() != null) ? component.getStyle().getName() : null;
   if (style != null) {
       %><span class="<%=style%>"><%
   }
   %><%|name=component.getName()%><%
   %><%|target=component.getTarget()%><%
   %><%|href=component.getReference()%><%
%>><%
   Utils.renderContainer(device, component);
%></a>
</write>
</template>
