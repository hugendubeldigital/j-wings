<template name="LineBreak" for="org.wings.SLineBreak">

<write>
<br<% 
Utils.printStyle(device, component.getStyle());
switch (component.getClear()) {
   case SLineBreak.CLEAR_ALL  : %> clear="all"<%; break;
   case SLineBreak.CLEAR_LEFT : %> clear="left"<%; break;
   case SLineBreak.CLEAR_RIGHT: %> clear="right"<%; break;
};%> />
</write>
</template>
