<template name="SpacerCG" for="org.wings.SSpacer">
<write>
<%
  if (component.getOrientation() == VERTICAL) {
       for ( int i=component.getSpaces(); i > 0 ; --i) {
          %><br/><%
       }
  }
  else {
       for ( int i=component.getSpaces(); i > 0 ; --i) {
          %>&nbsp;<%
       }
  }
%></write>
</template>
