<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
  "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>

  <!-- loading the HelloWingSServlet -->
  <servlet>
    <servlet-name>WingSSkeletonServlet</servlet-name>
    <servlet-class>skeleton.WingSSkeletonServlet</servlet-class>
  </servlet>

  <!-- the servlet container shall map the HelloWingS -->
  <servlet-mapping>
    <servlet-name>WingSSkeletonServlet</servlet-name>
    <url-pattern>/WingSSkeletonServlet/*</url-pattern>
  </servlet-mapping>

  <!-- The Welcome File List -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>






















